<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.whatflower.ui.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/navigation_picture"
        android:name="com.example.whatflower.ui.PictureFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_user" />
    <fragment
        android:id="@+id/navigation_chats"
        android:name="com.example.whatflower.ui.ChatsFragment"
        android:label="@string/title_chats"
        tools:layout="@layout/fragment_chats" />
    <fragment
        android:id="@+id/navigation_me"
        android:name="com.example.whatflower.ui.UserFragment"
        android:label="@string/title_me"
        tools:layout="@layout/fragment_picture" />

</navigation>